<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>requestServlet</servlet-name>
		<servlet-class>roi.students.t3t.server.ServerRequestImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>requestServlet</servlet-name>
		<url-pattern>t3t/req</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>t3t.html</welcome-file>
	</welcome-file-list>

